@{
}
@model Models.Trans

<turbo-stream action="replace" target="transaction_@Model.Id">
    <template>
        @if (Model.Id > 0)
        {
            <partial name="_WalletTransaction" model="@Model"></partial>
        }else
        {
            <turbo-frame id="transaction_@Model.Id"></turbo-frame>
        }
        
    </template>
</turbo-stream>